{
  "parser": "babel-eslint",
  "rules": {
    "no-var": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "comma-dangle": [2, "never"],
    "semi": [2, "always"],
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-cond-assign": [2, "always"],
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [0, "both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "curly": [2, "all"],
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
    "dot-location": [2, "property"],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 0,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-proto": 2,
    "no-redeclare": [2, {"builtinGlobals": true}],
    "no-return-assign": [2, "always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "wrap-iife": 2,
    "yoda": [2, "never", {
      "exceptRange": false,
      "onlyEquality": false
    }],
    "strict": [2, "never"],
    "no-delete-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [0, "nofunc"],
    "array-bracket-spacing": [2, "never"],
    "brace-style": 2,
    "camelcase": [2, {"properties": "never"}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "eol-last": 2,
    "func-style": [0, "expression"],
    "id-length": [0, {"min": 2, "exceptions": ["i", "e"]}],
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true
    }],
    "lines-around-comment": [2, {
      "beforeBlockComment": true,
      "beforeLineComment": true ,
      "allowBlockStart": true
    }],
    "linebreak-style": [2, "unix"],
    "new-cap": [2, {
      "capIsNewExceptions": [
        "Immutable.List",
        "Immutable.Map",
        "Immutable.OrderedMap",
        "Immutable.Set",
        "Immutable.OrderedSet",
        "Immutable.Stack",
        "Immutable.Range",
        "Immutable.Repeat",
        "Immutable.Record",
        "Immutable.KeyedSeq",
        "Immutable.SetSeq",
        "Immutable.Iterable",
        "Immutable.KeyedIterable",
        "Immutable.IndexedIterable",
        "Immutable.SetIterable",
        "Immutable.Collection",
        "Immutable.KeyedCollection",
        "Immutable.IndexedCollection",
        "Immutable.SetCollecion"
        ]
    }],
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2, "never"],
    "one-var": [2, "never"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": [0, "never"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single", "avoid-escape"],
    "semi-spacing": [2, {"before": false, "after": false}],
    "keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, {"anonymous": "never", "named": "never"}],
    "space-in-parens": [2, "never"],
    "space-infix-ops": [2, {"int32Hint": false}],
    "space-unary-ops": [1, { "words": true, "nonwords": false }],
    "spaced-comment": [2, "always"],
    "arrow-spacing": [2, { "before": true, "after": true }],
    "generator-star-spacing": [2, "before"],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 2,
    "object-shorthand": [2, "always"],
    "prefer-const": 0,
    "prefer-spread": 2,
    "require-yield": 2
  },
  "globals": {
    "_": true,
    "global": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "process": true,
    "before": true
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
